{
    "version": "1",
    "name": "InstaCloser",
    "launch_path": "/index.html",
    "description": "See instagram photos taken around you based in your geolocation.",
    "icons": {
        "128": "/img/instacloser-icon128",
        "60": "/img/instacloser-icon60.png"
    },
    "developer": {
        "name": "Felquis Gimenes",
        "url": "http://felquis.com"
    },
    "fullscreen": "true",
    "installs_allowed_from": ["*"],
    "default_locale": "en",
    "permissions": {
        "geolocation": {
            "description" : "Marking out user location"
        }
    }
}
